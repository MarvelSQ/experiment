{
  "name": "@marvelsq/experiment",
  "version": "0.4.1",
  "description": "personal experiment",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "browser": "lib/index.js",
  "files": [
    "esm",
    "cjs"
  ],
  "scripts": {
    "build:esm": "tsc --target es6 --module es2015 --outDir esm",
    "build:cjs": "tsc --target es6 --module commonjs --outDir cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "docs:build": "vite build --config docs/vite.config.js",
    "docs:dev": "vite --config docs/vite.config.js",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarvelSQ/experiment.git"
