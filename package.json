  },
  "homepage": "https://github.com/MarvelSQ/experiment#readme",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.22.1",
    "date-fns": "^2.29.2",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-html": "^3.2.0",
    "vitest": "^0.22.1"
  }
